msgstr "advarsel:"

#: quack.py:115
msgid "[installed]"
msgstr "[installert]"

#: quack.py:140
msgid "Orphaned packages"
msgstr "Avsondrede pakker"

#: quack.py:143
msgid "no orphaned package found"
msgstr "fant ingen avsondrede pakker"

#: quack.py:145
msgid "Pacman post transaction files"
msgstr "Etteroverføringsfiler for Pacman"

#: quack.py:158
msgid "no transactional file found"
msgstr "ingen overgangsfil funnet"

#: quack.py:164
msgid "Removed packages kept in cache"
msgstr "Fjernede pakker beholdt i hurtiglager"

#: quack.py:169
msgid "Old package versions kept in cache"
msgstr "Gamle pakkeversjoner beholdt i hurtiglager"

#: quack.py:223
#, python-brace-format
msgid "Your system run a newer version of {pkg}"
msgstr "Systemet ditt kjører en nyere versjon av {pkg}"

#: quack.py:244
msgid "Do you want to upgrade the above packages?"
msgstr "Ønsker du å oppgradere pakkene ovenfor?"

#: quack.py:255
#, python-brace-format
msgid "{pkg} is NOT an AUR package"
msgstr "{pkg} er IKKE en AUR-pakke"

#: quack.py:267
#, python-brace-format
msgid "impossible to clone {pkg} from AUR"
msgstr "kunne ikke klone {pkg} fra AUR"

#: quack.py:273
#, python-brace-format
msgid "Package {pkg} is ready to be built in {path}"
msgstr "Pakken {pkg} er klar til å bli bygd i {path}"

#: quack.py:296
msgid "A copy of the built packages has been kept in /tmp."
msgstr "En kopi av de bygde pakkene har blitt behold i /tmp."

#: quack.py:309
msgid "You should REALLY take time to inspect its PKGBUILD."
msgstr "Du burde VIRKELIG ta deg tid til å gjennomse dens PKGBUILD."

#: quack.py:310
msgid "When it's done, shall we continue?"
msgstr "Når det er gjort, skal vi fortsette?"

#: quack.py:361
msgid "The following packages have been built:"
msgstr "Følgende pakker har blitt bygd:"

#: quack.py:366
msgid "Which one do you really want to install?"
msgstr "Hvilken ønsker du å installere?"

#: quack.py:382
#, python-brace-format
msgid "{str} is not a valid input"
msgstr "{str} er ikke gyldig inndata"

#: quack.py:447
msgid "Name"
msgstr "Navn"

#: quack.py:448
msgid "Version"
msgstr "Versjon"

#: quack.py:449
msgid "Description"
msgstr "Beskrivelse"

#: quack.py:450
msgid "URL"
msgstr "Nettadresse"

#: quack.py:451
msgid "Licenses"
msgstr "Lisenser"

#: quack.py:452
msgid "Provides"
msgstr "Tilbyr"

#: quack.py:453
msgid "Depends On"
msgstr "Avhenger av"

#: quack.py:454
msgid "Build Depends On"
msgstr "Bygg avhenger av"

#: quack.py:455
msgid "Conflicts With"
msgstr "Kolliderer med"

